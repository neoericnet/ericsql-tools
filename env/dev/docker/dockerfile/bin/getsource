#!/bin/bash

export SHELL_PATH=$(dirname $0; pwd)
source $HOME/bin/env

cd $HOME/source
#clone mysql
if [ ! -d "mysql" ]; then
  git clone $mysql_repos mysql
else
  cd mysql
  git pull
fi
chmod 777 $HOME/source/mysql

#clone mysql-tools
cd $HOME/source
if [ ! -d "mysql-tools" ]; then
  git clone $mysql_tools_repos mysql-tools
else
  cd mysql-tools
  git pull
fi
chmod 777 $HOME/source/mysql-tools
